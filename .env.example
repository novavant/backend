ENV=production
PORT=8080

# CORS - tambahkan origin frontend jika 403/500 (contoh: http://localhost:3000,https://novavant.com)
CORS_ALLOWED_ORIGINS=

#Database connection
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=vlauser
DB_PASS=vlapass
DB_ROOT_PASSWORD=vlaroot
DB_NAME=vla-db
DB_TLS=false

#Redis connection
REDIS_ADDR=redis:6379
REDIS_PASS=your_redis_password_here
REDIS_DB=0

# Cloudflare R2 (S3-compatible storage)
# Buat API token di: Cloudflare Dashboard > R2 > Manage R2 API Tokens
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=your_bucket_name

#Server key
JWT_SECRET=your_jwt_secret_minimum_32_characters
CRON_KEY=your_cron_secret_key
SF_API_KEY=your_sf_api_key

# Pakailink SNAP (VA & QRIS)
PAKAILINK_CLIENT_KEY=
PAKAILINK_CLIENT_SECRET=
PAKAILINK_PARTNER_ID=
PAKAILINK_PRIVATE_KEY_PATH=./keys/pakailink.pem
PAKAILINK_PAYMENT_CALLBACK_URL=https://api.novavant.com/v3/callback/payments
PAKAILINK_PAYOUT_CALLBACK_URL=https://api.novavant.com/v3/callback/payouts
PAKAILINK_MERCHANT_ID=
PAKAILINK_STORE_ID=
PAKAILINK_TERMINAL_ID=
# Optional: PAKAILINK_BASE_URL (default: https://api.pakailink.id)

TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_BOT_USERNAME=novavant_cs_bot
TELEGRAM_ALLOWED_GROUP_IDS=-1001234567890
GROQ_API_KEY=your_groq_api_key
GROQ_MODEL=llama-3.1-70b-versatile

#Fazzpass
FAZPASS_MERCHANT_KEY=your_merchant_key
FAZPASS_GATEWAY_KEY=your_gateway_key

# Optional: full DSN (overrides DB_HOST/PORT/USER/PASS/NAME if set)
# Example for Docker: root:123456789@tcp(db:3306)/v1?charset=utf8mb4&parseTime=True&loc=Local
DB_DSN=

# JWT audience and issuer (optional, but recommended)
JWT_AUD=
JWT_ISS=